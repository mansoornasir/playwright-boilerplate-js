{
  "name": "playwright-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "https://mansoornasir.github.io/playwright-boilerplate-js",
  "scripts": {
    "test": "rimraf allure-results && rimraf allure-report && cucumber-js",
    "allure:generate": "npx allure generate",
    "allure:open": "allure open ./allure-report",
    "html:report": "node ./src/utils/cucumber_html_report_generator.js",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,css,html,js}\" ",
    "only": "npx cucumber-js --tags @only",
    "smoke": "npx cucumber-js --tags @smoke",
    "test:parallel": "cucumber-js --parallel=2",
    "test:chromium": "BROWSER=chromium cucumber-js",
    "test:firefox": "BROWSER=firefox cucumber-js",
    "test:webkit": "BROWSER=webkit cucumber-js",
    "test:parallel2": "npm-run-all --parallel test:chromium test:firefox test:webkit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cucumber/cucumber": "^10.9.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.46.1",
    "allure-commandline": "^2.30.0",
    "allure-cucumberjs": "^3.0.0-beta.10",
    "allure-js-commons": "^3.0.0-beta.10",
    "allure-playwright": "^3.0.0-beta.9",
    "cucumber-html-reporter": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "3.3.3",
    "prettier-eslint": "^16.3.0",
    "rimraf": "^5.0.10",
    "testrail-api-client": "^1.0.30"
  }
}
